{
  "name": "@sk-web-gui/ai",
  "version": "3.0.1",
  "sideEffects": false,
  "license": "MIT",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "typings": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts",
      "default": "./dist/esm/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "start": "nodemon --watch src --exec yarn build -e ts,tsx",
    "build": "yarn build:types && yarn build:esm && yarn build:cjs",
    "test": "jest --env=jsdom --passWithNoTests",
    "lint": "concurrently yarn:lint:*",
    "version": "yarn build",
    "build:esm": "swc src -d dist/esm --config-file ../../.swcrc.esm && node ../../fix-esm-extensions.mjs ./dist/esm",
    "build:cjs": "swc src -d dist/cjs --config-file ../../.swcrc",
    "build:types": "tsc -p ./tsconfig.production.json --emitDeclarationOnly --declaration --declarationDir dist/types",
    "test:cov": "yarn test --coverage",
    "lint:src": "eslint src"
  },
  "devDependencies": {
    "react": "^19.0.0"
  },
  "dependencies": {
    "@microsoft/fetch-event-source": "^2.0.1",
    "@sk-web-gui/accordion": "2.1.9",
    "@sk-web-gui/avatar": "1.2.2",
    "@sk-web-gui/button": "2.1.8",
    "@sk-web-gui/divider": "1.1.6",
    "@sk-web-gui/forms": "2.6.1",
    "@sk-web-gui/icon": "3.1.2",
    "@sk-web-gui/link": "1.2.7",
    "@sk-web-gui/navigationbar": "3.0.0",
    "@sk-web-gui/theme": "2.6.0",
    "@sk-web-gui/tooltip": "1.1.2",
    "@sk-web-gui/utils": "2.2.2",
    "@types/sanitize-html": "^2.16.0",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.537.0",
    "microsoft-cognitiveservices-speech-sdk": "^1.45.0",
    "react-markdown": "^10.1.0",
    "regenerator-runtime": "^0.14.1",
    "sanitize-html": "^2.17.0",
    "universal-cookie": "^8.0.1",
    "usehooks-ts": "^3.1.1",
    "zustand": "^5.0.7"
  },
  "peerDependencies": {
    "react": ">=18.3.1"
  }
}
