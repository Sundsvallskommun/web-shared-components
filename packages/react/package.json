{
  "name": "@sk-web-gui/react",
  "version": "3.0.2",
  "sideEffects": false,
  "license": "MIT",
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "typings": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts",
      "default": "./dist/esm/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "start": "nodemon --watch src --exec yarn build -e ts,tsx",
    "build": "yarn build:types && yarn build:esm && yarn build:cjs",
    "test": "jest --env=jsdom --passWithNoTests",
    "lint": "concurrently yarn:lint:*",
    "version": "yarn build",
    "build:esm": "swc src -d dist/esm --config-file ../../.swcrc.esm && node ../../fix-esm-extensions.mjs ./dist/esm",
    "build:cjs": "swc src -d dist/cjs --config-file ../../.swcrc",
    "build:types": "tsc -p ./tsconfig.production.json --emitDeclarationOnly --declaration --declarationDir dist/types",
    "test:cov": "yarn test --coverage",
    "lint:src": "eslint src"
  },
  "devDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "dependencies": {
    "@sk-web-gui/accordion": "2.1.9",
    "@sk-web-gui/avatar": "1.2.2",
    "@sk-web-gui/badge": "1.2.0",
    "@sk-web-gui/breadcrumb": "1.1.7",
    "@sk-web-gui/button": "2.1.8",
    "@sk-web-gui/callout": "1.2.2",
    "@sk-web-gui/card": "1.3.1",
    "@sk-web-gui/chip": "1.2.3",
    "@sk-web-gui/cookie-consent": "2.1.9",
    "@sk-web-gui/divider": "1.1.6",
    "@sk-web-gui/filter": "1.2.9",
    "@sk-web-gui/forms": "2.6.1",
    "@sk-web-gui/icon": "3.1.2",
    "@sk-web-gui/image": "1.1.6",
    "@sk-web-gui/label": "1.1.2",
    "@sk-web-gui/layout": "2.0.1",
    "@sk-web-gui/link": "1.2.7",
    "@sk-web-gui/list": "1.2.7",
    "@sk-web-gui/logo": "1.2.4",
    "@sk-web-gui/menu-vertical": "2.2.8",
    "@sk-web-gui/modal": "2.3.1",
    "@sk-web-gui/navigationbar": "3.0.0",
    "@sk-web-gui/pagination": "1.1.9",
    "@sk-web-gui/popup-menu": "3.1.8",
    "@sk-web-gui/progress-bar": "1.1.2",
    "@sk-web-gui/progress-stepper": "1.3.0",
    "@sk-web-gui/searchfield": "2.3.9",
    "@sk-web-gui/snackbar": "2.2.7",
    "@sk-web-gui/spinner": "1.1.7",
    "@sk-web-gui/table": "3.2.9",
    "@sk-web-gui/tabs": "1.4.4",
    "@sk-web-gui/text": "1.1.7",
    "@sk-web-gui/theme": "2.6.0",
    "@sk-web-gui/tooltip": "1.1.2",
    "@sk-web-gui/user-menu": "1.1.8",
    "@sk-web-gui/utils": "2.2.2"
  },
  "peerDependencies": {
    "react": ">=19.0.0",
    "react-dom": ">=19.0.0"
  }
}
