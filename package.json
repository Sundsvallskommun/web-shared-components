{
  "name": "sk-web-gui",
  "private": true,
  "version": "1.0.0",
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "description": "",
  "main": "",
  "directories": {},
  "scripts": {
    "clean": "lerna clean --yes && rimraf node_modules",
    "clean-dist": "yarn lerna exec rimraf dist",
    "boot": "yarn && yarn build",
    "boot:web": "yarn && yarn build:web",
    "reboot": "yarn clean-dist | yarn cache clean && yarn && yarn build:nocache",
    "dev": "yarn dev-storybook",
    "lerna-watch": "lerna watch -- lerna run build --verbose --scope=%LERNA_PACKAGE_NAME%",
    "build": "lerna run build --no-private --stream",
    "build:web": "lerna run build:esm,build:types --no-private --stream",
    "build:types": "lerna run build:types",
    "build:nocache": "yarn build --skip-nx-cache",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "yarn storybook",
    "storybook": "start-storybook -p 6006 -s public",
    "dev-storybook": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build -o ./storybook-static",
    "release": "changeset publish",
    "link-all": "yarn unlink-all && lerna exec --parallel yarn link",
    "unlink-all": "lerna exec --parallel --bail=false yarn unlink",
    "lint": "eslint ./packages",
    "prepare": "husky"
  },
  "resolutions": {
    "**/react": "^18.2.0",
    "**/react-dom": "^18.2.0"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.23.9",
    "@babel/core": "^7.24.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/runtime": "^7.24.0",
    "@changesets/cli": "^2.27.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@storybook/addon-a11y": "^7.6.7",
    "@storybook/addon-essentials": "^7.6.7",
    "@storybook/builder-vite": "^7.6.7",
    "@storybook/cli": "^7.6.7",
    "@storybook/react": "^7.6.7",
    "@storybook/react-vite": "^7.6.7",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "autoprefixer": "^10.4.18",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^9.0.11",
    "install": "^0.13.0",
    "lerna": "^7.4.1",
    "npm": "^10.5.0",
    "postcss": "^8.4.36",
    "postcss-import": "^16.0.1",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "sass": "^1.72.0",
    "storybook": "7.6.7",
    "storybook-dark-mode": "^3.0.3",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.2",
    "vite": "^5.1.6"
  },
  "dependencies": {
    "react-hook-form": "^7.51.1"
  },
  "dependenciesComments": {
    "vite": "Wait for this until upgrade: https://github.com/storybookjs/storybook/issues/25127"
  }
