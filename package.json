{
  "name": "sk-web-gui",
  "private": true,
  "version": "1.0.0",
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "description": "",
  "main": "index.js",
  "directories": {},
  "scripts": {
    "watch": "node watcher.mjs",
    "dev": "concurrently \"node watcher.mjs\" \"yarn storybook\" ",
    "clean": "lerna clean --yes && rimraf node_modules",
    "clean-dist": "yarn lerna exec rimraf dist",
    "prestart": "yarn && yarn bootstrap",
    "boot": "yarn prestart && yarn build",
    "start": "yarn build && yarn storybook",
    "start:all": "yarn build && concurrently \"yarn storybook\" ",
    "build": "lerna run build --no-private --stream",
    "build:nocache": "yarn build --skip-nx-cache",
    "bootstrap": "yarn install && lerna bootstrap --use-workspaces",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "start-storybook -p 6006 --no-open",
    "build-storybook": "build-storybook",
    "release": "changeset publish",
    "link-all": "yarn unlink-all && lerna exec --parallel yarn link",
    "unlink-all": "lerna exec --parallel --bail=false yarn unlink",
    "lint": "eslint ."
  },
  "resolutions": {
    "**/react": "^18.1.0",
    "**/react-dom": "^18.1.0"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.18.6",
    "@babel/core": "^7.19.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.18.6",
    "@changesets/cli": "^2.23.0",
    "@storybook/addon-a11y": "^6.5.9",
    "@storybook/addon-console": "^1.2.3",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/builder-webpack5": "^6.5.10",
    "@storybook/manager-webpack5": "^6.5.10",
    "@storybook/react": "^6.1.21",
    "@tailwindcss/forms": "^0.5.2",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.2",
    "addon-screen-reader": "^1.8.6",
    "autoprefixer": "^10.4.7",
    "chokidar": "^3.5.3",
    "concurrently": "^7.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.1",
    "eslint-plugin-storybook": "^0.6.4",
    "install": "^0.13.0",
    "lerna": "^5.1.6",
    "npm": "^8.13.2",
    "postcss": "^8.4.14",
    "prettier": "^2.4.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "storybook-addon-outline": "^1.4.2",
    "storybook-addon-performance": "^0.16.1",
    "tailwindcss": "^3.1.4",
    "typescript": "^4.7.4",
    "watcher": "^1.2.0"
  }
}
