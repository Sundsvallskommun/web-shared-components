{
  "name": "sk-web-gui",
  "private": true,
  "version": "1.0.0",
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "description": "",
  "main": "",
  "directories": {},
  "scripts": {
    "clean": "lerna clean --yes && rimraf node_modules",
    "clean-dist": "yarn lerna exec rimraf dist",
    "boot": "yarn && yarn build",
    "boot:web": "yarn && yarn build:web",
    "dev": "concurrently --kill-others \"yarn dev-storybook\" \"yarn lerna-watch\"",
    "lerna-watch": "lerna watch -- lerna run build --verbose --scope=%LERNA_PACKAGE_NAME%",
    "build": "lerna run build --no-private --stream",
    "build:web": "lerna run build:esm,build:types --no-private --stream",
    "build:types": "lerna run build:types",
    "build:nocache": "yarn build --skip-nx-cache",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "yarn storybook",
    "storybook": "start-storybook -p 6006 -s public",
    "dev-storybook": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build -o ./storybook-static",
    "release": "changeset publish",
    "link-all": "yarn unlink-all && lerna exec --parallel yarn link",
    "unlink-all": "lerna exec --parallel --bail=false yarn unlink",
    "lint": "eslint ."
  },
  "resolutions": {
    "**/react": "^18.2.0",
    "**/react-dom": "^18.2.0"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.2",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/preset-env": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@babel/preset-typescript": "^7.23.2",
    "@babel/runtime": "^7.23.2",
    "@changesets/cli": "^2.26.2",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@storybook/addon-a11y": "^7.5.1",
    "@storybook/addon-essentials": "^7.5.1",
    "@storybook/builder-vite": "^7.5.1",
    "@storybook/cli": "^7.5.1",
    "@storybook/react": "^7.5.1",
    "@storybook/react-vite": "^7.5.1",
    "@tailwindcss/forms": "^0.5.6",
    "@types/react": "^18.2.31",
    "@types/react-dom": "^18.2.14",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.52.0",
    "eslint-plugin-storybook": "^0.6.15",
    "install": "^0.13.0",
    "lerna": "^7.4.1",
    "npm": "^10.2.1",
    "postcss": "^8.4.31",
    "prettier": "^3.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "sass": "^1.69.4",
    "storybook": "7.5.1",
    "storybook-dark-mode": "^3.0.1",
    "tailwindcss": "3.3.4",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-commonjs": "^0.10.0",
