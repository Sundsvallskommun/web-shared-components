{
  "name": "sk-web-gui",
  "private": true,
  "version": "1.0.0",
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "description": "",
  "main": "",
  "directories": {},
  "scripts": {
    "clean": "lerna clean --yes && rimraf node_modules",
    "clean:dist": "yarn lerna exec rimraf dist",
    "boot": "yarn && yarn build:nocache",
    "boot:esm": "yarn && yarn build:esm:nocache",
    "boot:storybook": "yarn && lerna run build:esm --no-private --stream --skip-nx-cache && yarn build:storybook",
    "reboot": "yarn clean:dist | yarn cache clean && yarn boot",
    "dev": "yarn dev-storybook",
    "dev:test": "yarn dev-storybook --test",
    "lerna-watch": "lerna watch -- lerna run build --verbose --scope=%LERNA_PACKAGE_NAME%",
    "build": "cross-env SWC_CACHE=1 yarn build:types && yarn build:esm && yarn build:cjs",
    "build:nocache": "cross-env SWC_CACHE=0 nx reset && yarn build:types && yarn build:esm:nocache && yarn build:cjs:nocache",
    "build:lerna": "lerna run build --no-private --stream",
    "build:lerna:nocache": "cross-env SWC_CACHE=0 nx reset && yarn build:lerna --skip-nx-cache",
    "build:types": "lerna run build:types --no-private --stream",
    "build:esm": "lerna run build:esm --no-private --stream",
    "build:esm:nocache": "yarn build:esm --skip-nx-cache",
    "build:cjs": "lerna run build:cjs --no-private --stream",
    "build:cjs:nocache": "yarn build:cjs --skip-nx-cache",
    "build:storybook": "storybook build -o ./storybook-static",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "yarn storybook",
    "storybook": "storybook dev -p 6006",
    "storybook:prod": "http-server storybook-static",
    "dev-storybook": "cross-env NODE_ENV=development storybook dev -p 6006 --no-open",
    "release": "changeset publish",
    "link-all": "yarn unlink-all && lerna exec --parallel yarn link",
    "unlink-all": "lerna exec --parallel --bail=false yarn unlink",
    "lint": "eslint ./packages",
    "prepare": "husky"
  },
  "resolutions": {
    "**/react": "^18.3.1",
    "**/react-dom": "^18.3.1"
  },
  "license": "MIT",
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@storybook/addon-a11y": "8.6.14",
    "@storybook/addon-essentials": "8.6.14",
    "@storybook/cli": "8.6.14",
    "@storybook/react": "8.6.14",
    "@storybook/react-vite": "8.6.14",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "eslint": "^9.32.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "eslint-plugin-storybook": "^0.12.0",
    "glob": "^11.0.3",
    "husky": "^9.1.7",
    "install": "^0.13.0",
    "lerna": "8.2.3",
    "npm": "^11.5.2",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "prettier": "^3.6.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "rimraf": "^6.0.1",
    "sass": "^1.90.0",
    "storybook": "8.6.14",
    "storybook-dark-mode": "4.0.2",
    "swc": "^1.0.11",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.0",
    "vite": "7.1.1",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "dependencies": {
    "dotenv": "^17.2.1",
    "http-server": "^14.1.1",
    "react-hook-form": "^7.62.0"
  },
  "repository": "https://github.com/Sundsvallskommun/web-shared-components.git",
  "author": "Sundsvallskommun"
}
