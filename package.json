{
  "name": "sk-web-gui",
  "private": true,
  "version": "1.0.0",
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "description": "",
  "main": "",
  "directories": {},
  "scripts": {
    "clean": "lerna clean --yes && rimraf node_modules",
    "clean-dist": "yarn lerna exec rimraf dist",
    "prestart": "yarn && yarn bootstrap",
    "boot": "yarn prestart && yarn build",
    "start": "yarn dev-storybook",
    "dev": "yarn dev-storybook",
    "lerna-watch": "yarn lerna -- watch -- lerna run build --since",
    "start:all": "yarn build && concurrently \"yarn storybook\" ",
    "build": "lerna run build --no-private --stream",
    "build:types": "lerna run build:types",
    "build:nocache": "yarn build --skip-nx-cache",
    "bootstrap": "yarn install && lerna bootstrap --use-workspaces",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev-storybook": "storybook dev -p 6006 --no-open",
    "build-storybook": "storybook build",
    "release": "changeset publish",
    "link-all": "yarn unlink-all && lerna exec --parallel yarn link",
    "unlink-all": "lerna exec --parallel --bail=false yarn unlink",
    "lint": "eslint ."
  },
  "resolutions": {
    "**/react": "^18.2.0",
    "**/react-dom": "^18.2.0"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.21.0",
